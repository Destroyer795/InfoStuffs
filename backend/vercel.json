{
  "version": 2,
  "builds": [
    {
      "src": "api/info.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/info/(.*)",
      "dest": "api/info.js"
    },
    {
      "src": "/api/info",
      "dest": "api/info.js"
    },
    {
      "src": "/(.*)",
      "dest": "api/info.js"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Credentials", "value": "true" },
        { "key": "Access-Control-Allow-Origin", "value": "https://info-stuffs.vercel.app" },
        { "key": "Access-Control-Allow-Methods", "value": "GET,POST,PUT,DELETE,PATCH,OPTIONS" },
        { "key": "Access-Control-Allow-Headers", "value": "Content-Type, Authorization, X-Requested-With" }
      ]
    }
  ]
}
